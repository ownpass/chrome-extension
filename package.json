{
  "name": "ownpass-chrome-extension",
  "version": "1.0.0",
  "description": "The OwnPass Chrome extension.",
  "keywords": [
    "chrome",
    "ownpass",
    "password"
  ],
  "author": {
    "name": "Walter Tamboere",
    "email": "walter@tamboer.nl",
    "url": "http://waltertamboer.nl"
  },
  "license": "proprietary",
  "scripts": {
    "build_prod": "npm run build && browserify -s main build/ng-compiled/main.js > build/ng-compiled/ownpass-chrome-extension.js && npm run minify",
